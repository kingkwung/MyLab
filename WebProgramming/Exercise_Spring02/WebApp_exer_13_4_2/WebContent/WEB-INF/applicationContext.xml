						<property name="lastName" value="Sungjin" />
						<property name="balance" value="-200000.0" />
					</bean>
				</entry>
				<entry key="a1236">
					<bean class="myPackage.Customer">
						<property name="id" value="a1236" />
						<property name="firstName" value="Lee" />
						<property name="lastName" value="JinHwan" />
						<property name="balance" value="5.0" />
					</bean>
				</entry>

			</map>
		</property>
	</bean>
</beans>